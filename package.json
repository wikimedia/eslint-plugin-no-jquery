  "repository": {
    "type": "git",
    "url": "git+https://github.com/wikimedia/eslint-plugin-no-jquery.git"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "index.js",
